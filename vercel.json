{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@datacrm_api_url",
    "NEXTAUTH_URL": "@datacrm_frontend_url",
    "NEXTAUTH_SECRET": "@datacrm_nextauth_secret"
  }
}
